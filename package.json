{
  "name": "stretto",
  "version": "1.0.16",
  "description": "Stretto is a high-performance TypeScript library for streaming HTTP requests with support for Server-Sent Events.",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "browser": "./dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "cp": "cp dist/index.umd.js docs/",
    "build": "vite build && npm run cp",
    "dev": "vite build --minify false && npm run cp",
    "clean": "rm -rf dist",
    "test": "jest tests/index.test.ts",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "dts-bundle-generator": "^9.5.1",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "fetch api",
    "fetch stream",
    "streaming",
    "sse",
    "HTTP request",
    "Server-Sent Events"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wutility/stretto.git"
  },
  "bugs": {
    "url": "https://github.com/wutility/stretto.git/issues"
  },
  "homepage": "https://github.com/wutility/stretto.git#readme",
  "author": "Haikel Fazzani",
  "license": "MIT"
}
