{
  "name": "stretto",
  "version": "1.0.5",
  "description": "Stretto is a high-performance TypeScript library for streaming HTTP requests with support for Server-Sent Events.",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "browser": "./dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "cp": "cp dist/index.umd.js docs/",
    "build": "vite build && npm run cp",
    "dev": "vite build --minify false && npm run cp",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "deno": "deno run --unstable-sloppy-imports --allow-net tests/index.test.ts"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "fetch api",
    "streaming",
    "sse",
    "HTTP request",
    "Server-Sent Events"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wutility/stretto.git"
  },
  "bugs": {
    "url": "https://github.com/wutility/stretto.git/issues"
  },
  "homepage": "https://github.com/wutility/stretto.git#readme",
  "author": "Haikel Fazzani",
  "license": "MIT"
}
